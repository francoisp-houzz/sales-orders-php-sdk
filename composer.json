{
  "name": "SalesOrders",
  "description": "PHP library for Vendasta's SalesOrders service",
  "type": "library",
  "require": {
    "grpc/grpc": "^v1.1.0",
    "guzzlehttp/guzzle": "^6.3",
    "mdanter/ecc": "^0.5.0",
    "google/protobuf": "^3.6.0",
    "vendasta/vax": "^1.2.0"
  },
  "autoload": {
    "psr-4": {
      "Vendasta\\SalesOrders\\V1\\": "./src",
      "Salesorders\\V1\\": "./generated/grpc/SalesOrders/V1",
      "Sales\\V1\\": "./generated/grpc/Sales/V1",
      "GPBMetadata\\MarketplaceApps\\V1\\": "./generated/grpc/GPBMetadata/MarketplaceApps/V1",
      "GPBMetadata\\Sales\\V1\\": "./generated/grpc/GPBMetadata/Sales/V1",
      "GPBMetadata\\SalesOrders\\V1\\": "./generated/grpc/GPBMetadata/SalesOrders/V1",
      "GPBMetadata\\VendastaTypes\\": "./generated/grpc/GPBMetadata/VendastaTypes"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  }
}
